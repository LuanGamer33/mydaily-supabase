{
    "rewrites": [
        { "source": "/", "destination": "/index.html" },
        { "source": "/dashboard", "destination": "/dashboard.html" },
        { "source": "/habits", "destination": "/habits.html" },
        { "source": "/events", "destination": "/events.html" },
        { "source": "/notes", "destination": "/notes.html" },
        { "source": "/activities", "destination": "/activities.html" },
        { "source": "/settings", "destination": "/settings.html" },
        { "source": "/(.*)/.html", "destination": "/$1" }
    ],
    "trailingSlash": false,
    "cleanUrls": true
}
