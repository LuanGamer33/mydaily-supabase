{
    "rewrites": [
        { "source": "/", "destination": "/index.html" },
        { "source": "/dashboard", "destination": "/dashboard.html" },
        { "source": "/habitos", "destination": "/habits.html" },
        { "source": "/eventos", "destination": "/events.html" },
        { "source": "/notas", "destination": "/notes.html" },
        { "source": "/actividades", "destination": "/activities.html" },
        { "source": "/configuraciones", "destination": "/settings.html" },
        { "source": "/(.*)/.html", "destination": "/$1" }
    ],
    "trailingSlash": false,
    "cleanUrls": true
}
